#!/usr/bin/env python3
"""
Launches a web browser with the latest documentation for UCVM.

Copyright:
    Southern California Earthquake Center

Developer:
    David Gill <davidgil@usc.edu>
"""
# Python Imports
import webbrowser

# UCVM Imports
from ucvm.src.framework.ucvm import UCVM

webbrowser.open_new(
    UCVM.get_replacement_string("http://hypocenter.usc.edu/research/ucvm/[version]/docs/index.html")
)
